
if ($("#calendar").length != 0) {
  $("#calendar").append("<%= escape_javascript(render partial: 'period_form', locals: {period: @period, start_at: @start_at, doctor_id: @doctor_id, changed_by: @changed_by, period_id: @period_id, agency_id: @agency_id}) %>")
}
else if ($("#default_calendar").length != 0) {
  $("#default_calendar").append("<%= escape_javascript(render partial: 'period_form', locals: {period: @period, start_at: @start_at, doctor_id: @doctor_id, changed_by: @changed_by, period_id: @period_id, agency_id: @agency_id}) %>")
}
else if ($("#custom_calendar").length != 0) {
  $("#custom_calendar").append("<%= escape_javascript(render partial: 'period_form', locals: {period: @period, start_at: @start_at, doctor_id: @doctor_id, changed_by: @changed_by, period_id: @period_id, agency_id: @agency_id}) %>")
}
if ($("form").length > 1) {
  $("form").first().remove()
  $('#cal_event').find(".modal-body").append($("form"))
}
$('#cal_event').modal('show')